{
  "name": "luoluo-vue-components",
  "type": "module",
  "version": "0.0.4",
  "description": "`luoluo` components, provide a simple encapsulation of ElementPlus, used to reduce duplicate code in the `yyhhenry` series projects.",
  "license": "MIT",
  "exports":{
    ".": "./lib/lib.js"
  },
  "types": "./lib.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build -c lib.config.ts",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vueuse/core": "^10.6.1",
    "element-plus": "^2.4.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.18.10",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
